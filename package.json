{
  "name": "safemate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:preprod": "vite --mode preprod",
    "dev:production": "vite --mode production",
    "start": "vite --mode development",
    "build": "tsc -b && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "build:dev": "vite build --mode development",
    "build:preprod": "vite build --mode preprod",
    "build:production": "vite build --mode production",
    "deploy:dev": "npm run build:dev && aws s3 sync dist/ s3://dev-safemate-static-hosting",
    "deploy:preprod": "npm run build:preprod && aws s3 sync dist/ s3://preprod-safemate-static-hosting",
    "deploy:production": "npm run build:production && aws s3 sync dist/ s3://prod-safemate-static-hosting"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.11.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.12",
    "@mui/material": "^6.4.12",
    "@mui/x-date-pickers": "^8.5.3",
    "@radix-ui/react-slot": "^1.2.3",
    "amazon-cognito-identity-js": "^6.3.15",
    "aws-amplify": "^6.15.4",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "notistack": "^3.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-react": "^4.4.1",
    "buffer": "^6.0.3",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.0",
    "process": "^0.11.10",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
